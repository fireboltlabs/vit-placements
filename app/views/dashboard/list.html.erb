<div class="col-lg-10">
<center><h2>Registered Students</h2></center><br>
<div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th><%=sort_link @q, :student_regd_no, "Register No"%></th>
                  <th>Full Name</th>
                  <th><%= sort_link @q, :btech_branch, "Branch"%></th>
                  <th>Email Id</th>
                  <th>Mobile</th>
                  <th><%= sort_link @q, :ssc_perc, "SSC" %></th>
                  <th><%= sort_link @q, :inter_or_diplomo_perc, "Inter or Diplomo"%></th>
                  <th><%= sort_link @q, :btech_total_perc, "B.Tech" %></th>
                  <th>Backlogs</th>
                </tr>
              </thead>
              <tbody>
              <% @students.each do |student|%>
                <tr>
                  <td><%= student.regd_no %></td>

                  <td><%= student.first_name+" "+student.middle_name+" "+
                  student.last_name%></td>

                  <td><%= student.btech.branch %></td>

                  <td><%= student.email %></td>

                  <td><%= student.mobile %></td>

                  <td><%= student.ssc.perc %></td>

                  <td><%= student.inter_or_diplomo.perc %></td>

                  <td><%= student.btech.total_perc %></td>

                  <td><%= student.btech.active_backlogs %></td>
                </tr>
              <% end %>
              </tbody>
        </table>
</div>
</div>